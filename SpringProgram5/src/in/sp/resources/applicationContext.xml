<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean class="in.sp.beans.Address" id="addId">
        <property name="houseno" value="203"/>
        <property name="city" value="fbd"/>
        <property name="pincode" value="121004"/>
    </bean>
    <bean class="in.sp.beans.Student" id="stdId">
        <property name="rollNo" value="101"/>
        <property name="name" value="taran"/>
        <property name="address" ref="addId"/>
    </bean>

</beans>